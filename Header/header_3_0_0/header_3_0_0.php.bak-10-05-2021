          <script>
            Element.prototype.hasClass=function(e){return this.className&&new RegExp("(^|\\s)"+e+"(\\s|$)").test(this.className)};const iconMenu=document.getElementById("icon-menu"),menuShow=document.getElementById("menu-show"),bgShow=document.getElementById("bg-menu"),iconSubMenu=document.getElementsByClassName("header_4_2_2__box__item"),iconRight=document.getElementsByClassName("menu-icon");if(screen.width<1100){var i;for(i=0;i<iconSubMenu.length;i++)iconSubMenu[i].addEventListener("click",function(){this.classList.toggle("active");var e=this.nextElementSibling;"block"===e.style.display?e.style.display="none":e.style.display="block"});if(window.screen.width<1050)for(const e of iconRight)e.addEventListener("click",function(){e.hasClass("icon-angle-down")?(e.classList.add("icon-angle-up"),e.classList.remove("icon-angle-down")):(e.classList.remove("icon-angle-up"),e.classList.add("icon-angle-down"))});iconMenu.addEventListener("click",function(){menuShow.classList.add("active"),bgShow.classList.add("active")}),bgShow.addEventListener("click",function(){menuShow.classList.remove("active"),bgShow.classList.remove("active")})}!function(){var e,n=document.documentElement,t=window,s=t.scrollY||n.scrollTop,i=0,o=0,c=document.getElementById("header_4_2_2"),l=function(e,n){2===e&&n>52?(c.classList.add("hide"),o=e):1===e&&(c.classList.remove("hide"),o=e)};window.addEventListener("scroll",function(){(e=t.scrollY||n.scrollTop)>s?i=2:e<s&&(i=1),i!==o&&l(i,e),s=e})}();         
          </script>
      ';
